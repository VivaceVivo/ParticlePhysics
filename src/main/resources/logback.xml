<?xml version="1.0" encoding="UTF-8"?>

<configuration debug="true" scan="true" scanPeriod="10 seconds">

	
<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
	<encoder>
	<pattern>%d{HH:mm:ss,SSS} %-5p %c{3} %M %L  %m%n</pattern>
	</encoder>
</appender>




<logger name="particlephysics">
	<level value="info" />
</logger>

<logger name="particlephysics.container.ParticleBox2D">
       <level value="info" />
</logger>

<logger name="particlephysics.ParticleBox">
      <level value="info" />
</logger>

<root>
	<level value="warn"/>
	<appender-ref ref="STDOUT"/>
</root>

</configuration>
